                    <div class="container">

                        <!-- Page-Title -->
                        <div class="row">
                            <div class="col-sm-12">
                                <h4 class="pull-left page-title">Membres Table</h4>
                                <ol class="breadcrumb pull-right">
                                    <li><a href="#">Moltran</a></li>
                                    <li><a href="#">Tables</a></li>
                                    <li class="active">Membres Table</li>
                                </ol>
                            </div>
                        </div>


                        <div class="panel">
                            
                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-sm-6">
                                        <div class="m-b-30">
                                            <a asp-action="New" class="btn btn-primary waves-effect waves-light">Ajouter <i class="fa fa-plus"></i></a>
                                        </div>
                                    </div>
                                </div>
                                <table class="table table-bordered table-striped" id="datatable-editable">
                                    <thead>
                                        <tr>
                                            <th>@Html.DisplayNameFor(model => model.ProfilePicture)</th>
                                            <th>@Html.DisplayNameFor(model => model.Prenom)</th>
                                            <th>@Html.DisplayNameFor(model => model.Nom)</th>
                                            <th>@Html.DisplayNameFor(model => model.Sexe)</th>
                                            <th>@Html.DisplayNameFor(model => model.Data_Naiss)</th>
                                            <th>@Html.DisplayNameFor(model => model.Lieu_Naiss)</th>
                                            <th>@Html.DisplayNameFor(model => model.Nationalite)</th>
                                            <th>@Html.DisplayNameFor(model => model.Num_telephone)</th>
                                            <th>@Html.DisplayNameFor(model => model.Adresse)</th>
                                            <th>@Html.DisplayNameFor(model => model.Profession)</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr class="gradeX">
                                             @foreach (var membre in Model)
                                            {
                                        <tr>
                                            <td>
                                                <img src="~/images/@membre.ProfilePicture"
                                                     class="thumb-md img-circle"
                                                     height="40" width="40"
                                                     asp-append-version="true" />
                                            </td>
                                            <td>@membre.Prenom</td>
                                            <td>@membre.Nom</td>
                                            <td>@membre.Sexe</td>
                                            <td>@membre.Data_Naiss</td>
                                            <td>@membre.Lieu_Naiss</td>
                                            <td>@membre.Nationalite</td>
                                            <td>@membre.Num_telephone</td>
                                            <td>@membre.Adresse</td>
                                            <td>@membre.Num_telephone</td>
                                            <td class="actions">
                                                <a asp-action="Edit" asp-route-id="@membre.Id" class="on-default edit-row"><i class="fa-file-text-o"></i></a>
                                            </td>
                                        </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                            <!-- end: page -->

                        </div> <!-- end Panel -->

                    </div> <!-- container -->
                               
