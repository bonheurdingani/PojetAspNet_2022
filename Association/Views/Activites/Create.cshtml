                                                            </div>
                                                            <div class="external-event bg-purple" data-class="bg-purple" style="position: relative;">
                                                                <i class="fa fa-move"></i>My Event Four
                                                            </div> 
                                                        </div>

                                                        <!-- checkbox -->
                                                        <div class="checkbox checkbox-primary m-t-40">
                                                            <input id="drop-remove" type="checkbox">
                                                            <label for="drop-remove">
                                                                Remove after drop
                                                            </label>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div> <!-- end col-->
                                </div>  <!-- end row -->

                                <!-- BEGIN MODAL -->
                                <div class="modal fade" id="event-modal">
                                    <div class="modal-dialog">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                                                <h4 class="modal-title"><strong>Manage</strong> my events</h4>
                                            </div>
                                            <div class="modal-body"></div>
                                            <div class="modal-footer">
                                                <button type="button" class="btn btn-default waves-effect" data-dismiss="modal">Close</button>
                                                <button type="button" class="btn btn-success save-event waves-effect waves-light">Create event</button>
                                                <button type="button" class="btn btn-danger delete-event waves-effect waves-light" data-dismiss="modal">Delete</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!-- Modal Add Category -->
                                <div class="row">
                                <div class="modal fade" id="add-category">
                                    <div class="modal-dialog">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                                                <h4 class="modal-title"><strong>Ajouter</strong> Activite</h4>
                                            </div>
                                            <div class="modal-body">
                                                <form asp-action="Create">
                                                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                                    <input type="hidden" asp-for="Id" />
                                                        <div class="col-md-6">
                                                            <label asp-for="Nom_association" class="control-label">Nom_association</label>
                                                            <input asp-for="Nom_association" class="form-control form-white" />
                                                            <span asp-validation-for="Nom_association" class="text-danger"></span>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <label asp-for="Date_activite" class="control-label">Date activite </label>
                                                            <input asp-for="Date_activite" class="form-control form-white" />
                                                            <span asp-validation-for="Date_activite" class="text-danger"></span>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <label asp-for="Libelle" class="control-label">Libelle Name</label>
                                                            <input asp-for="Libelle" class="form-control form-white" />
                                                            <span asp-validation-for="Libelle" class="text-danger"></span>
                                                        </div>
                                                  </form>
                                                  <div class="modal-footer">
                                                            <button type="button" class="btn btn-default waves-effect" data-dismiss="modal">Close</button>
                                                            <input type="submit" class="btn btn-primary" value="Create"/>
                                                        </div>
                                                   @section Scripts {
                                                       @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
                                                   }
                                              </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- END MODAL -->
                            </div>
